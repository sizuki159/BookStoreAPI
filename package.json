    "@types/source-map-support": "^0.5.7",
    "adonis-preset-ts": "^2.1.0",
    "pino-pretty": "^10.2.0",
    "typescript": "~4.6",
    "youch": "^3.3.1",
    "youch-terminal": "^2.2.2"
  },
  "dependencies": {
    "@adonisjs/auth": "^8.2.3",
    "@adonisjs/core": "^5.9.0",
    "@adonisjs/lucid": "^18.4.0",
    "@adonisjs/repl": "^3.1.11",
    "adonis-lucid-soft-deletes": "^1.4.4",
    "luxon": "^3.4.3",
    "mysql2": "^3.6.1",
    "phc-bcrypt": "^1.0.8",
