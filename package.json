{
  "name": "book-store-api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "fresh": "node ace migration:fresh --seed",
    "dev": "node ace serve --watch",
    "build": "node ace build --production --ignore-ts-errors",
    "start": "pm2 start server.js",
    "env": "cp .env_deployment build/.env",
